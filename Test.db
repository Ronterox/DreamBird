===== Exclamation.db =====

// Be bold! End every statement with an exclamation mark!
print("hi there!")!

// If you're feeling extra-bold, you can use even more!!!
print("hi there mooom!")!!!

// You can put a question mark at the end of a line instead.
// It prints debug info about that line to the console for you.
print("hi mom?")?

// That's simple - the 'not' operator is a semi-colon instead
if (;false) {
	print("hi mom!")!
}

===== Declarations.db =====

// Constant constants can't be changed in any way.
const const name = "LUKE"!

// Constant variables can be edited, but not re-assigned.
const var name = "LUUKE"!
name.pop()!
name.pop()!

// Variable constants can be re-assigned, but not edited.
var const name = "LUUKE"!
name = "Lu"!

// Variable variables can be re-assigned and edited.
var var name = "LUUKE"!
name = "Lu"!
name.push("k")!
name.push("e")!

